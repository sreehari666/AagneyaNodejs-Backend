<style>
table {
  font-family: arial, sans-serif;
  border-collapse: collapse;
  width: 100%;
}

td, th {
  border: 1px solid #dddddd;
  text-align: left;
  padding: 8px;
}

tr:nth-child(even) {
  background-color: #dddddd;
}
.button {
  border-radius: 30px;
  color: white;
  background: linear-gradient(to right,#ff105f,#ffad06);

  padding: 15px 32px;
  text-align: center;
  text-decoration: none;
  display: inline-block;
  font-size: 16px;
  margin: 4px 2px;
  cursor: pointer;
}
.home {
  background-color: #f1f1f1;
  color: black;
}

a {
  text-decoration: none;
  display: inline-block;
  padding: 8px 16px;
}

a:hover {
  background-color: #ddd;
  color: black;
}
</style>
<a href="/admin" class="home">&laquo; Back to Home</a>
<center><h1>Judges</h1></center>
<table>
  <tr>
    <th>Name</th>
    <th>Email</th>
    <th>Password</th>
   
    
  </tr>
  {{#each newList}}
  <tr>
    <td>{{this.name}}</td>
    <td>{{this.email}}</td>
    <td>{{this.password}}</td>
    
    
{{/each}}
    
    
  </tr>
  
  
  </tr>
</table>